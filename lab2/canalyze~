 
#include <stdio.h>
#include <stdlib.h>
#include <time.h>
  
 int main(int argc, char **argv) {
	FILE *ret;
	char txt[128]="1";
	char tempWord[128]="1";
	int i,j;
	

	ret = fopen( argv[1], "r"); /*ret is a pointer to the file*/
	
	while(fgets(txt, 128, ret)){
		fgets(txt, 128, ret);	
		for(i=0; txt[i]!=0; ++i){
		  tempWord[0]=0;
		  for(j=0; txt[j]!=' '; ++j){
		    tempWord=tempWord+txt[j];
	}


	printf("%s", txt);
	fclose (ret);

	return 0;
  }
